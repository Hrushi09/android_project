<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="367dp"
        android:layout_height="208dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/banjara" />

    <TextView
        android:id="@+id/krishna"
        android:layout_width="158dp"
        android:layout_height="38dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="127dp"
        android:text="Taj krishna"
        android:textAlignment="center"
        android:textColor="#FBFBFB"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10" />

    <ImageView
        android:id="@+id/wifi3"
        android:layout_width="35dp"
        android:layout_height="36dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="23dp"
        app:layout_constraintEnd_toStartOf="@+id/wifitxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:srcCompat="@drawable/wifi" />

    <ImageView
        android:id="@+id/rstrt3"
        android:layout_width="34dp"
        android:layout_height="38dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="18dp"
        app:layout_constraintEnd_toStartOf="@+id/rstrttxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi3"
        app:srcCompat="@drawable/restaurant" />

    <ImageView
        android:id="@+id/prkng3"
        android:layout_width="34dp"
        android:layout_height="43dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="19dp"
        app:layout_constraintEnd_toStartOf="@+id/prkngtxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rstrt3"
        app:srcCompat="@drawable/parking" />

    <ImageView
        android:id="@+id/bar3"
        android:layout_width="39dp"
        android:layout_height="36dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/bartxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prkng3"
        app:srcCompat="@drawable/bar" />

    <TextView
        android:id="@+id/wifitxt"
        android:layout_width="129dp"
        android:layout_height="22dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="39dp"
        android:layout_marginBottom="42dp"
        android:text="Wifi"
        android:textSize="16sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/rstrttxt"
        app:layout_constraintStart_toEndOf="@+id/wifi3"
        app:layout_constraintTop_toBottomOf="@+id/krishna" />

    <TextView
        android:id="@+id/rstrttxt"
        android:layout_width="131dp"
        android:layout_height="21dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="171dp"
        android:text="Restaurant"
        android:textSize="16sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rstrt3"
        app:layout_constraintTop_toBottomOf="@+id/wifitxt" />

    <TextView
        android:id="@+id/prkngtxt"
        android:layout_width="130dp"
        android:layout_height="28dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="171dp"
        android:text="Parking"
        android:textSize="16sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/prkng3"
        app:layout_constraintTop_toBottomOf="@+id/rstrttxt" />

    <TextView
        android:id="@+id/bartxt"
        android:layout_width="131dp"
        android:layout_height="25dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="168dp"
        android:text="Bar"
        android:textSize="16sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bar3"
        app:layout_constraintTop_toBottomOf="@+id/prkngtxt" />

    <Button
        android:id="@+id/reserve3"
        android:layout_width="102dp"
        android:layout_height="51dp"
        android:layout_marginStart="265dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#43A047"
        android:text="Reserve"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>